	<system.web>
		<!-- 增加最大请求长度 (以KB为单位) -->
		<httpRuntime maxRequestLength="20480" />
		<!-- 例如，20MB -->
	</system.web>
	<system.webServer>
		<!-- 增加允许的文件上传大小 (以字节为单位) -->
		<security>
			<requestFiltering>
				<requestLimits maxAllowedContentLength="20971520" />
				<!-- 例如，20MB -->
			</requestFiltering>
		</security>
	</system.webServer>
</configuration>
